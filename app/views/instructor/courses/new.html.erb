<br />
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Add a new course</h1>
  <%= simple_form_for @course, url: instructor_courses_path do |f| %><br />
      <%= f.input :title %><br />
      <%= f.input :description %><br />
      <%= f.input :cost %><br />
      <br />
      <%= f.submit "Create Course", class: 'btn btn-primary' %>
  <% end %>
  <br />
</div>